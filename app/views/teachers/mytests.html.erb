   <p id="notice"><%= notice %></p>
<%= render 'sweetify/alert' %>
<h1>My Tests</h1>


<div class="row">
                             <div class="col-sm-12">
                                <div class="card-box">
                                
                                    <table id="datatable" class="table table-striped table-bordered">
                                            <thead>
    <tr>
      <th>Id</th>
      <th>Title</th>
      <th>Subject</th>
      <th>Objective</th>
      <th class="col-xs-3 col-sm-2">Action</th>
    </tr>
  </thead>


                                            <tbody>
    <% @tests.each do |test| %>
      <tr>
        <td><%=test.id%></td>
        <td><%= test.title %></td>
        <td><%=Subject.find(test.subject_id).sub_name%></td>
        <td><%= test.objective %></td>
        <td><div class="col-sm-4"><%= link_to 'Show', test %></div>
        <div class="col-sm-4"><%= link_to edit_test_path(test) do%><i class="fa fa-pencil"></i>   <% end %></div>
        <%= link_to test, method: :delete, data: { confirm: 'Are you sure?' } do%><i class="fa fa-trash"></i> <% end %></td>
      </tr>
    <% end %>
  </tbody>
                                        </table>
<%= link_to '/tests/new', :class => 'btn btn-default' do%><i class="fa fa-plus"></i> New Test<% end %><br/>
                                </div>
                            </div>
                        </div>

<br>

