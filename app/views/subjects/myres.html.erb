 <div class="row">
														<div class="col-sm-12">
																<h4 class="page-title">Blank Page</h4>
																<ol class="breadcrumb">
																		<li>
																				<a href="#">Ubold</a>
																		</li>
																		<li>
																				<a href="#">Pages</a>
																		</li>
																		<li class="active">
																				Blank Page
																		</li>
																</ol>
														</div>
												</div>

<% @class.subjects.each do |a| %>
												<div class="card-box">
														<h4 class="header-title m-t-0"><b><%= a.sub_name %></b></h4>
														<p class="text-muted m-b-30 font-13">Subject description</p>
						<% a.tests.each do |b| %>
														<div class="portlet">
																<div class="portlet-heading portlet-default">
																		<h3 class="portlet-title text-dark">
																				Default Heading Attemps: 3 | Overall average: 80%
																		</h3>
																																				<div class="portlet-widgets">
																				<a href="javascript:;" data-toggle="reload"><i class="ion-refresh"></i></a>
																				<span class="divider"></span>
																				<a data-toggle="collapse" data-parent="#accordion1" href="#<%=b.id%>" class="collapsed" aria-expanded="false">details</a>
																				<span class="divider"></span>

																		</div>
																		<div class="clearfix"></div>
																</div>
																<div id="<%=b.id%>" class="panel-collapse collapse" aria-expanded="false" style="height: 0px;">
																		<div class="portlet-body">
																	   <table class="table table-striped table-bordered">
										<thead>
											<tr>
												<th data-field="id" data-sortable="true" data-formatter="invoiceFormatter">Order ID</th>
												
												<th data-field="date" data-sortable="true" data-formatter="dateFormatter">Last touched</th>
												<th data-field="amount" data-align="center" data-sortable="true" data-sorter="priceSorter">Mark</th>
												<th data-field="status" data-align="center" data-sortable="true" data-formatter="statusFormatter">Status</th>
											</tr>
										</thead>
										
										<tbody>
										<%Evaluation.where(student_id: current_user.id, test_id: b.id).each do |c|%>
											<tr>
												<td><%=c.id%></td>
												<td><%=c.updated_at%></td>
												<td><%=c.score%></td>
												
												<%if !c.complete? %>
												<td><span class="label label-table label-warning">Inomplete</span></td>
												<%elsif c.complete?%>
												<td><span class="label label-table label-success">Complete</span></td>
												<%else%>
												<%end%>
											</tr>
											
											<%end%>
										
										</tbody>
									</table> 
																		</div>

																</div>

														</div>
<%end%>
												</div>
<%end%>