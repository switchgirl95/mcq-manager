<p id="notice"><%= notice %></p>

<h1>Fokeys</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fokeys.each do |fokey| %>
      <tr>
        <td><%= link_to 'Show', fokey %></td>
        <td><%= link_to 'Edit', edit_fokey_path(fokey) %></td>
        <td><%= link_to 'Destroy', fokey, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fokey', new_fokey_path %>
