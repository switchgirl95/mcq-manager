 
<fieldset class="nested-fields">


  <div class="col-md-10 col-md-offset-1">
																<div class="panel panel-border panel-custom">
																		<div class="panel-heading">
																				<h3 class="panel-title">Question <%= f.options[:child_index] +1 %></h3>
																		</div>

																		<div class="panel-body">
																				<p>
																				
  																				<%= f.object.question %><br />
																				</p><hr/>

  <div class= "answers">
  <%= f.fields_for :eanswers do |builder| %>
    <%= render 'eanswer_fields', f: builder %>
  <% end %>
  </div>
  </div>
  </div>
  </div>
  
</fieldset>
